CREATE TABLE CATES
(
  id INT NOT NULL PRIMARY KEY AUTO_INCREMENT
  ,NAME VARCHAR(30) NOT NULL
  ,BREED VARCHAR(30) NOT NULL
  ,AGE INT
  );
  INSERT INTO CATES(NAME, BREED, AGE)
VALUES ('Ringo', 'Tabby', 4),
